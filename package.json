{
  "name": "natours-api",
  "version": "1.0.0",
  "description": "natours backend api building",
  "type": "module",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "nodemon server.js",
    "start:prod": "nodemon start.js",
    "debug": "ndb server.js",
