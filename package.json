    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "mongo-sanitize": "^1.1.0",
